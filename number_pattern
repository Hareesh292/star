num=5
for val in range(num):
    for val in range(1,num+1):
        print(val,end=' ')
    print()
print('--------------------------')
num=5
for val in range(1,num+1):
    for vl in range(num):
        print(val,end=' ')
    print()
print('------------------------------')
num=5
for val in range(1,num+1):
    for vv in range(1,val+1):
        print(val,end=' ')
    print()
print('-------------------------')
num=5
for val in range(num,0,-1):
    for vv in range(num):
        print(val,end=' ')
    print()
