num=5
for val in range(num,0,-1):
    for vv in range(val):
        print(val,end=' ')
    print()
print('-----------------------')
num=5
for val in range(num,0,-1):
    for vv in range(num-val+1):
        print(val,end=' ')
    print()
print('-------------------')
num=5
for val in range(1,num+1):
    for va in range(num-val):
        print(' ',end=' ')
    for vv in range(val):
        print(val,end=' ')
    print()
print('-------------------------------')
